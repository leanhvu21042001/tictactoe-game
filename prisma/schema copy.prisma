datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id        String   @id @default(cuid())
  board     String   @default("000000000")
  status    String   @default("WAITING")
  player1Id String?
  player2Id String?
  winnerId  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
